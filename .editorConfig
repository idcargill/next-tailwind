# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true

[**.{js,json,md,ts,tsx}]
indent_style = space
indent_size = 2
insert_final_newline = true
quote_type = single
# traling_comma = 'es5'